   "source": [
    "# Load PGL libraries and start a PGL window\n",
    "from pgl import pgl\n",
    "pgl = pgl()\n",
    "\n",
    "# close any existing windows\n",
    "pgl.cleanUp()\n",
    "\n",
    "# open a new window\n",
    "pgl.open(0,800,600)\n",
    "pgl.visualAngle(57, 60,40)"
   ]
  }
